* {
  border: none;
  border-radius: 10px;
  font-family: 'JetbrainsMono Nerd Font';
  font-size: 15px;
  min-height: 10px;
}

window#waybar {
  background-color: transparent;
  border-radius: 15px;
  margin-top: 10px;
}

window#waybar.hidden {
  opacity: 0.2;
}

#window {
  padding-left: 10px;
  padding-right: 10px;
  border-radius: 10px;
  transition: none;
  color: transparent;
  background: transparent;
}

#workspaces {
  margin: 6px 6px 6px 6px;
  border-radius: 18px;
  background: #222330;
  transition: none;
  padding-left: 0px;
  padding-right: 0px;
}

#workspaces button {
  transition: none;
  color: #12A376;
  background: transparent;
  margin-right: 10px;
}

#workspaces button.persistent {
  transition: none;
  color: #7aa2f7;
  background: transparent;
  font-size: 4px;
}

#workspaces button.empty {
  color: #12A376;
  font-size: 2px;
  transition: none;
  background: transparent;
}

#workspaces button.active {
  transition: none;
  color: #e0af68;
  background: transparent;
  font-size: 4px;
}

#network {
  margin-top: 6px;
  margin-left: 6px;
  padding-left: 10px;
  padding-right: 10px;
  margin-bottom: 6px;
  border-radius: 18px;
  font-size: 18px;
  font-weight: bold;
  transition: none;
  color: #e0af68;
  background: #222330;
}

#wireplumber {
  margin-top: 6px;
  margin-left: 8px;
  padding-left: 10px;
  padding-right: 10px;
  margin-bottom: 6px;
  border-radius: 18px;
  font-size: 18px;
  font-weight: bold;
  transition: none;
  color: #f1f1f1;
  background: #222330;
}

@keyframes blink {
  to {
    color: #b5e8e0;
  }
}

#backlight {
  margin-top: 6px;
  margin-left: 8px;
  padding-left: 10px;
  padding-right: 10px;
  margin-bottom: 0px;
  border-radius: 10px;
  transition: none;
  color: #161320;
  background: #f8bd96;
}

#clock {
  margin-top: 6px;
  margin-left: 8px;
  padding-left: 10px;
  padding-right: 10px;
  margin-right: 6px;
  margin-bottom: 6px;
  border-radius: 18px;
  transition: none;
  font-size: 18px;
  font-weight: bold;
  color: #f1f1f1;
  background: #222330;
}

#cpu {
  margin-top: 6px;
  margin-left: 8px;
  padding-left: 10px;
  margin-bottom: 6px;
  padding-right: 10px;
  border-radius: 18px;
  font-size: 18px;
  font-weight: bold;
  transition: none;
  color: #f1f1f1;
  background: #222330;
}

#memory {
  margin: 6px 0px 6px 8px;
  padding-left: 10px;
  padding-right: 10px;
  border-radius: 18px;
  font-size: 18px;
  font-weight: bold;
  transition: none;
  color: #f1f1f1;
  background: #222330;
}

#disk {
  margin-top: 6px;
  margin-left: 8px;
  padding-left: 10px;
  margin-bottom: 6px;
  padding-right: 10px;
  border-radius: 18px;
  font-size: 18px;
  font-weight: bold;
  transition: none;
  color: #f1f1f1;
  background: #222330;
}

#tray {
  margin-top: 6px;
  margin-left: 8px;
  padding-left: 10px;
  margin-bottom: 6px;
  padding-right: 10px;
  border-radius: 10px;
  transition: none;
  color: #b5e8e0;
  background: #161320;
}
